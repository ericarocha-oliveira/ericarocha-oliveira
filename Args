def multiplicar (*args):
  total = 1
  for numero in args:
    total *= numero
  return total

multiplicação = multiplicar(10, 2, 3, 4, 5)
print (multiplicação)
print (10*2*3*4*5)

def par_imp (numero):
  return numero % 2 == 0

print (par_imp (4))
print (par_imp (5))
print (par_imp (10))
print (par_imp (13))
